@{
    List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByLot_Result> farmbylotList = (ViewBag.farmbylotList as List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByLot_Result>);

    List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByCategory_Result> farmbycategoryList = (ViewBag.farmbycategoryList as List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByCategory_Result>);
    List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_HF_Result> TotalData = (ViewBag.TotalData as List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_HF_Result>);
    decimal totalHarvest = 0;
    decimal totalForecast = 0;
    if (TotalData != null)
    {
        foreach (var item in TotalData)
        {
            totalHarvest = totalHarvest + Convert.ToDecimal(item.TotalPoundsHarvested);
            totalForecast = totalForecast + Convert.ToDecimal(item.TotalPoundsForecast);
        }
    }
    List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_Summary_Result> farmdashboardSummary = (ViewBag.farmdashboardSummary as List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_Summary_Result>);
    List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByStages_Result> farmdashboardByStages = (ViewBag.farmdashboardByStages as List<SmartAdminMvc.Models.DbEntity.USP_FarmDashboard_ByStages_Result>);
    decimal TotalPoundsHarvested = 0;
    decimal HarvestedLbsPercentage = 0;
    decimal EstimatedFutureHarvestLeft = 0;
    decimal TotalPoundsForecast = 0;
    decimal NumLotNoHarvest = 0;
    decimal TotalNumLotNo = 0;
    decimal PercentageLostHarvested = 0;
    decimal TotalNumCategory = 0;
    decimal NumCategoryHarvest = 0;
    decimal strainHarvestedPerc = 0;
    decimal NumLotNoVegetation = 0;
    decimal NumLotNoSeedling = 0;
    decimal NumLotNoFlowering = 0;
    decimal NumLotNoPreFlowering = 0;

    if (farmdashboardSummary != null)
    {
        foreach (var item in farmdashboardSummary)
        {
            TotalPoundsHarvested = TotalPoundsHarvested + Convert.ToDecimal(item.TotalPoundsHarvested);
            TotalPoundsForecast = TotalPoundsForecast + Convert.ToDecimal(item.TotalPoundsForecast);
            HarvestedLbsPercentage = HarvestedLbsPercentage + Convert.ToDecimal(item.HarvestedLbsPercentage);
            EstimatedFutureHarvestLeft = EstimatedFutureHarvestLeft + Convert.ToDecimal(item.EstimatedFutureHarvestLeft);
            NumLotNoHarvest = NumLotNoHarvest + Convert.ToDecimal(item.NumLotNoHarvest);
            TotalNumLotNo = TotalNumLotNo + Convert.ToDecimal(item.TotalNumLotNo);
            TotalNumCategory = TotalNumCategory + Convert.ToDecimal(item.TotalNumCategory);
            NumCategoryHarvest = NumCategoryHarvest + Convert.ToDecimal(item.NumCategoryHarvest);
            PercentageLostHarvested = (NumLotNoHarvest / TotalNumLotNo) * 100;
            strainHarvestedPerc = (NumCategoryHarvest / TotalNumCategory) * 100;
        }
    }
    if (farmdashboardByStages != null)
    {
        foreach (var item in farmdashboardByStages)
        {
            if (item.FarmFieldStatusName == "Seedling")
            {
                NumLotNoSeedling = NumLotNoSeedling + Convert.ToDecimal(item.NumLotNo);
            }
            if (item.FarmFieldStatusName == "Vegetation")
            {
                NumLotNoVegetation = NumLotNoVegetation + Convert.ToDecimal(item.NumLotNo);
            }
            if (item.FarmFieldStatusName == "Flowering")
            {
                NumLotNoFlowering = NumLotNoFlowering + Convert.ToDecimal(item.NumLotNo);
            }
            if (item.FarmFieldStatusName == "PreFlowering")
            {
                NumLotNoPreFlowering = NumLotNoPreFlowering + Convert.ToDecimal(item.NumLotNo);
            }

        }
    }
}
