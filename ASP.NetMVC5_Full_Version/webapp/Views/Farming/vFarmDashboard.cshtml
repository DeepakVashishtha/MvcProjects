                // do a random walk
                while (data.length < totalPoints) {
                    var prev = data.length > 0 ? data[data.length - 1] : 100;
                    var y = prev + Math.random() * 1000 - 5;
                  
                    data.push(y);
                }

                // zip the generated y values with the x values
